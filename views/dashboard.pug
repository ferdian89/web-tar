                  .summary-item Status: Janda
                  .summary-item Suku: Jawa
                  .summary-item Pendidikan: S1
                  .summary-item Pekerjaan: Karyawan swasta
                  .summary-item Bentuk badan: Normal (170/67)
                  .summary-item Warna Kulit: Putih
                  .summary-item Rambut: Lurus
              .card-body
                #headingOne
                  h5.mb-0
                    button.btn.btn-link.collapsed(type='button', data-toggle='collapse', data-target='#collapseOne', aria-expanded='true', aria-controls='collapseOne')
                #collapseOne.collapse(aria-labelledby='headingOne', data-parent='#accordionExample')
